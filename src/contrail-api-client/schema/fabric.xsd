        <xsd:element name="deploy-ramdisk" type="xsd:string"
                     description="UUID of the deploy initrd/ramdisk" />
        <xsd:element name="ipmi-password" type="xsd:string"
                     description="Password to access IPMI" />
        <xsd:element name="ipmi-port" type="xsd:string"
                     description="Remote IPMI RMCP port" />
        <xsd:element name="ipmi-username" type="xsd:string"
                     description="Username to access IPMI" />
        <xsd:element name="deploy-kernel" type="xsd:string"
