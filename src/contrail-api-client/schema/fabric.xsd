<xsd:complexType name="BaremetalProperties">
    <xsd:all>
        <xsd:element name="memory-mb" type="xsd:integer"
                     description="RAM of the Baremetal server (in MB)" />
        <xsd:element name="cpu-arch" type="xsd:string"
                     description="Architecture of the baremetal server" />
        <xsd:element name="local-gb" type="xsd:integer"
                     description="Disk size of root device (in GB)" />
        <xsd:element name="cpus" type="xsd:integer"
                     description="Number of CPU cores on the baremetal" />
        <xsd:element name="capabilities" type="xsd:string"
                     description="Boot option" />
    </xsd:all>
</xsd:complexType>

<xsd:complexType name="PortGroupProperties">
    <xsd:all>
        <xsd:element name="miimon" type="xsd:integer"
                     description="Specifies the MII link monitoring frequency in milliseconds." />
        <xsd:element name="xmit-hash-policy" type="xsd:string"
                     description="Transmit hash policy to use for slave selection in balance-xor, 802.3ad, and tlb modes." />
    </xsd:all>
</xsd:complexType>

<xsd:complexType name="BaremetalPortInfo">
    <xsd:all>
        <xsd:element name="pxe-enabled" type="xsd:boolean"
                     description="Indicates whether PXE is enabled or disabled on the Port." />
        <xsd:element name="local-link-connection" type="LocalLinkConnection"
                     description="The Port binding profile" />
        <xsd:element name="node-uuid" type="xsd:string"
                     description="UUID of the parent node where this port is connected" />
        <xsd:element name="address" type="xsd:string"
                     description="Mac Address of the NIC in Node" />
    </xsd:all>
</xsd:complexType>

<xsd:complexType name="CloudInstanceInfo">
    <xsd:all>
        <xsd:element name="os-version" type="xsd:string"
                     description="specific version of operating system" />
        <xsd:element name="operating-system" type="xsd:string"
                     description="Name of operating system, ex - centos7 or ubuntu16" />
        <xsd:element name="roles" type=""
                     description="Roles of the cloud instance" maxOccurs="unbounded"/>
        <xsd:element name="availability-zone" type="xsd:string"
                     description="Availability zone to which the instance belongs to" />
        <xsd:element name="instance-type" type="xsd:string"
                     description="size of instance" />
        <xsd:element name="machine-id" type="xsd:string"
                     description="image (ami) id of instance" />
        <xsd:element name="volume-size" type="xsd:integer"
                     description="Size of the volume for the cloud instance" />
    </xsd:all>
</xsd:complexType>

<xsd:complexType name="LocalLinkConnection">
    <xsd:all>
        <xsd:element name="switch-info" type="xsd:string"
                     description="UUID of the Physical-Port with contrail database" />
        <xsd:element name="port-index" type="xsd:string"
                     description="Port-Id provided by LLDP, This usually maps to SnmpIndex" />
        <xsd:element name="port-id" type="xsd:string"
                     description="Port ID of switch where Baremetal is connected" />
        <xsd:element name="switch-id" type="xsd:string"
                     description="Switch hostname" />
    </xsd:all>
</xsd:complexType>

<xsd:complexType name="ESXIHostInfo">
    <xsd:all>
        <xsd:element name="username" type="xsd:string"
                     description="username" />
        <xsd:element name="datacenter" type="xsd:string"
                     description="datacenter in vCenter" />
        <xsd:element name="esxi-name" type="xsd:string"
                     description="ESXI host name(ip)" />
        <xsd:element name="cluster" type="xsd:string"
                     description="cluster in vCenter" />
        <xsd:element name="mac" type="xsd:string"
                     description="mac address for contrailvm" />
        <xsd:element name="datastore" type="xsd:string"
                     description="datastore in vCenter" />
        <xsd:element name="password" type="xsd:string"
                     description="ESXI host password" />
        <xsd:element name="vcenter-server" type="xsd:string"
                     description="vCenter server" />
    </xsd:all>
</xsd:complexType>

